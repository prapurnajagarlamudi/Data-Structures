// circular queues using arrays

#include<stdio.h>
#define size 10

int q[size];
int rear=0,front=0;
int count=0;

void enqueue()
{
    int value;
    if(count==size)
    {
        printf("Queue is full\n");
    }
    else
    {
        printf("Enter the value to insert\n");
        scanf("%d",&value);
        q[rear]=value;
        rear=(rear+1)%size;
        count++;
    }
}

void dequeue()
{
    if(count==0 && rear==front)
    {
        printf("Queue is empty\n");
    }
    else
    {
        front=(front+1)%size;
        count--;
    }
}

void display()
{
    int i;
    if(rear==front && count==0)
    {
        printf("Queue is empty\n");
    }
    else if(front!=rear)
    {
        if(rear>front)
        {
            for(i=front;i<=rear;i++)
            {
                printf("%d\n",q[i]);
            }
        }
        if(rear<front)
        {
            for(i=front;i<size;i++)
            {
                printf("%d\n",q[i]);
            }
            for(i=0;i<rear;i++)
            {
                printf("%d\n",q[i]);
            }
        }
    }
    else if(front==rear && count!=0)
    {
        for(i=front;i<size;i++)
        {
            printf("%d\n",q[i]);
        }
    }
    else
    {
        return ;
    }
}

int main()
{
    int choice=0;
    while(choice!=3)
    {
        printf("Enter the choice");
        scanf("%d",&choice);
        switch(choice)
        {
            case 1: enqueue();
                    break;
            case 2: dequeue();
                    break;
            case 3: display();
                    break;
        }
    }
}

output:

Enter the choice1
Enter the value to insert
12
Enter the choice1
Enter the value to insert
22
Enter the choice1
Enter the value to insert
32
Enter the choice1
Enter the value to insert
42
Enter the choice2
Enter the choice3
22
32
42
0
