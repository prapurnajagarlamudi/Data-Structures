// linear search
#include<stdio.h>
void main ()
{
    int a[100],n,i,ele;
    printf("enter no of elements");
    scanf("%d",&n);
    printf("enter the elements");
    for (i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter the element to search");
    scanf("%d",&ele);
    for (i=0;i<n;i++)
    {
        if(a[i]==ele)
        {
            printf("%d is present at %d position",ele,i+1);
            break;
        }
    }
}
